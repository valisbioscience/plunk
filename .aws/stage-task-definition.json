{
    "ipcMode": null,
    "executionRoleArn": "arn:aws:iam::952221707682:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "plunk",
            "cpu": 0,
            "memory": 3072,
            "memoryReservation": 1024,
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": null,
                "options": {
                    "awslogs-group": "/ecs/plunk-stage",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "name": "plunk-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "command": [],
            "essential": true,
            "secrets": [
                {
                    "name": "REDIS_URL",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:REDIS_URL::"
                },
                {
                    "name": "DATABASE_URL",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:DATABASE_URL::"
                },
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:JWT_SECRET::"
                },
                {
                    "name": "AWS_REGION",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:AWS_REGION::"
                },
                {
                    "name": "AWS_SES_CONFIGURATION_SET",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:AWS_SES_CONFIGURATION_SET::"
                },
                {
                    "name": "NEXT_PUBLIC_API_URI",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:NEXT_PUBLIC_API_URI::"
                },
                {
                    "name": "API_URI",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:APP_URI::"
                },
                {
                    "name": "API_URI",
                    "valueFrom": "arn:aws:secretsmanager:eu-central-1:952221707682:secret:plunk-6ot96D:API_URI::"
                }
            ],
            "image": "952221707682.dkr.ecr.us-east-1.amazonaws.com/plunk"
        },
        {
            "name": "redis",
            "image": "public.ecr.aws/docker/library/redis:alpine3.20",
            "logConfiguration": {
                "logDriver": "awslogs",
                "secretOptions": null,
                "options": {
                    "awslogs-group": "/ecs/plunk-redsi",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379
                }
            ],
            "command": [
                "redis-server",
                "--dir",
                "/data"
            ],
            "environment": [
                {
                    "name": "ALLOW_EMPTY_PASSWORD",
                    "value": "yes"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "redisdata",
                    "containerPath": "/data",
                    "readOnly": false
                }
            ]
        }
    ],
    "family": "plunk-stage-task-definition",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "3072",
    "taskRoleArn": "arn:aws:iam::952221707682:role/plunk_ECS_TaskRole",
    "volumes": [
        {
            "name": "redisdata",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0446a354e80a6767e",
                "rootDirectory": "/"
            }
        }
    ]
}